<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>


<VBox minHeight="300.0" minWidth="500.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bordercrossing.controller.SuspendedController">
   <padding>
      <Insets bottom="60.0" left="60.0" right="60.0" top="60.0" />
   </padding>
   <children>
      <Label style="-fx-font-size: 14; -fx-font-weight: bold;" text="Suspended passengers:">
         <padding>
            <Insets bottom="20.0" />
         </padding>
      </Label>
      <ListView fx:id="suspendedListView">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ListView>
   </children>
</VBox>
